// Online C++ compiler to run C++ program online
#include <iostream>
#include<climits>
using namespace std;

int getmax(int num[],int n){
    int max=INT_MIN;
    for(int i=0;i<n;i++){
        if(num[i]>max){
            max=num[i];
        }
    }
    return max;
}

int main() {
    // Write C++ code here
    int size;
    cin>>size;
    int num[100];
    for(int i=0;i<size;i++){
        cin>>num[i];
    }
cout<<"maximum value is : "<<getmax(num,size)<<endl;
    
    

    return 0;
}
