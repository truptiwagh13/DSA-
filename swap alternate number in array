// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
void printarrya(int arr[],int n){
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
void swaparray(int arr[],int n){
    for(int i=0;i<n;i+=2){
        if(arr[i+1]<n){
            swap(arr[i],arr[i+1]);
        }
    }
}


int main() {
    int even[5]={2,3,4,5,6};
    swaparray(even,5);
    printarrya(even,5);
    return 0;
}
